{
  "name": "doc-mgt",
  "version": "1.0.0",
  "description": "A document management system",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack -p",
    "migration": "./node_modules/sequelize db:migrate",
    "start:selenium": "java -jar selenium-server-standalone-3.4.0.jar",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "e2e": "./node_modules/.bin/nightwatch --config nightwatch.js",
    "start": "./node_modules/.bin/webpack -p && babel-node server.js",
    "start:dev": "nodemon server.js --exec babel-node",
    "test:server": "NODE_ENV=test nyc --reporter=html --reporter=text mocha server/test/**/*.spec.js --compilers js:babel-register",
    "test:client": "NODE_ENV=test nyc --reporter=html --reporter=text --extension .jsx mocha  --compilers js:babel-core/register --require client/test/browser.js client/test/**/*.spec.{js,jsx}",
    "test": "npm run test:server && npm run test:client"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "server/test",
      "client/test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-ookwuolisa/doc-mgt.git"
  },
  "author": "obinna okwuolisa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-ookwuolisa/doc-mgt/issues"
  },
  "homepage": "https://github.com/andela-ookwuolisa/doc-mgt#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "env2": "^2.2.0",
    "enzyme": "^2.9.1",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "history": "^4.6.1",
    "jsdom": "^11.1.0",
    "jsonwebtoken": "^7.4.1",
    "materialize-css": "^0.98.2",
    "morgan": "^1.8.2",
    "nightwatch": "^0.9.16",
    "nock": "^9.0.13",
    "node-sass": "^4.5.3",
    "npmlog": "^4.1.2",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-paginate": "^4.4.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "selenium-download": "^2.0.10",
    "selenium-standalone": "^6.5.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "sinon": "^2.3.6",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "swagger-jsdoc": "^1.3.0",
    "sweetalert": "^1.1.3",
    "toastr": "^2.1.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.23.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "faker": "^4.1.0",
    "localStorage-mock": "^1.0.3",
    "mocha": "^3.4.2",
    "mock-local-storage": "^1.0.2",
    "moxios": "^0.4.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3"
  }
}
